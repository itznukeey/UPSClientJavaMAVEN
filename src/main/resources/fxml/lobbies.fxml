            <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refresh" text="Refresh">
               <font>
                  <Font name="Open Sans Regular" size="12.0"/>
               </font>
            </Button>
            <Button fx:id="joinLobbyButton" mnemonicParsing="false" text="Join Lobby">
               <font>
                  <Font name="Open Sans Regular" size="12.0"/>
               </font>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets right="20.0"/>
         </BorderPane.margin>
      </TilePane>
   </bottom>
   <center>
      <ListView fx:id="listView" prefHeight="661.0" prefWidth="868.0" stylesheets="@../css/styles.css"
                BorderPane.alignment="CENTER"/>
   </center>
</BorderPane>
